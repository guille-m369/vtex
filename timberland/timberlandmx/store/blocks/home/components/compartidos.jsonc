      "centerText": false,
      "ctaArray": [
        {
          "text": "VER MÁS",
          "url": "/473?map=productClusterIds"
        }
      ]
    }
  },

  // Seccion siluetas y estilos
  "flex-layout.row#imageTextImage_siluetasEstilos": {
    "children": ["three-block#siluetasEstilos"]
  },
  "three-block#siluetasEstilos": {
    "props": {
      "blockClass": "threeBlockItem",
      "firstImgUrl": "https://timberlandmx.vteximg.com.br/arquivos/tenis_silueta_leopardo_desktop.jpg",
      "secondImgUrl": "https://timberlandmx.vteximg.com.br/arquivos/tenis_silueta_normal_desktop.jpg",
