      "orderBy": "OrderByTopSaleDESC",
      "collection": "142"
    }
  },
  "slider-layout#shelf-product-care": {
    "props": {
      "showPaginationDots": "never",
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 3,
        "phone": 2
      },
      "infinite": true,
      "fullWidth": false,
      "blockClass": "shelf"
    }
  }
}
