{
  /*Item-------------------------------------*/
  "menu-item#desktop-header-menu-item-categorias": {
    "props": {
      "id": "desktop-header-menu-item-categorias",
      "type": "custom",
      //"iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
