{
    "flex-layout.row#contact": {
        "children": [
          "flex-layout.col#entrega-gratis",
          "flex-layout.col#24horas",
          "flex-layout.col#reembolso",
          "flex-layout.col#pago-seguro"
        ],
        "props": {
          "blockClass": "row-contacto",
          "justify": "center",
          "paddingTop": 8,
          "paddingBottom": 9
        }
      },
      "flex-layout.col#entrega-gratis": {
        "children": ["custom-infocard#entrega-gratis", "rich-text#title-infocard-envios-title", "rich-text#title-infocard-envios-subtitle"],
        "props": {
          "blockClass": "images",
          "verticalAlign": "middle"
        }
      },
      "flex-layout.col#24horas": {
        "children": [  "custom-infocard#24horas","rich-text#title-infocard-suporte-title", "rich-text#title-infocard-suporte-subtitle"],
